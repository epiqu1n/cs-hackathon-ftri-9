{
  "manifest_version": 3,
  "name": "Tab Janitor",
  "version": "0.1",
  "description": "Cleans up old tabs to keep your browser clean!",
  "permissions": ["storage", "tabs", "notifications", "activeTab", "scripting"],
  "background": {
    "service_worker": "/src/background.js"
  },
  "action": {
    "default_icon": {
      "16": "/src/img/icon_16.png",
      "32": "/src/img/icon_32.png",
      "64": "/src/img/icon_64.png",
      "128": "/src/img/icon_128.png"
    },
    "default_title": "Tab Janitor settings",
    "default_popup": "/src/popup.html"
  },
  "icons": {
    "16": "/src/img/icon_16.png",
    "32": "/src/img/icon_32.png",
    "64": "/src/img/icon_64.png",
    "128": "/src/img/icon_128.png"
  }
}